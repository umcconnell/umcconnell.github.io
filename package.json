{
    "name": "umcconnell.github.io",
    "version": "1.0.0",
    "description": "Just my personal blog.",
    "scripts": {
        "start": "npm run dev",
        "dev": "run-s clean eleventy:dev --print-label",
        "build": "run-s clean eleventy:prod --print-label",
        "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "eleventy:debug": "cross-env ELEVENTY_ENV=development DEBUG=Eleventy* eleventy --serve",
        "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "del-cli dist",
        "lint": "npx prettier --check .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/umcconnell/umcconnell.github.io"
    },
    "type": "module",
    "author": "Ulysse McConnell",
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy": "3.0.0-alpha.17",
        "@babel/core": "^7.25.2",
        "@babel/plugin-transform-runtime": "^7.24.7",
        "@babel/preset-env": "^7.25.3",
        "@iktakahiro/markdown-it-katex": "^4.0.1",
        "babel-loader": "^9.1.3",
        "clean-css": "^5.3.3",
        "critical": "^7.1.2",
        "cssesc": "^3.0.0",
        "del-cli": "^5.1.0",
        "focus-trap": "^7.5.4",
        "focus-visible": "^5.2.0",
        "fuse.js": "^7.0.0",
        "html-minifier": "^4.0.0",
        "luxon": "^3.4.4",
        "markdown-it": "^14.1.0",
        "markdown-it-anchor": "^9.0.1",
        "markdown-it-footnote": "^4.0.0",
        "markdown-it-table-of-contents": "^0.6.0",
        "memfs": "^4.11.1",
        "npm-run-all": "^4.1.5",
        "sass": "^1.77.8",
        "svg-sprite": "^2.0.4",
        "webpack": "^5.93.0"
    },
    "devDependencies": {
        "@11ty/eleventy-navigation": "^0.3.5",
        "@11ty/eleventy-plugin-rss": "^2.0.1",
        "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
        "cross-env": "^7.0.3",
        "prettier": "3.3.2"
    }
}
