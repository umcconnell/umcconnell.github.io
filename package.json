{
    "name": "umcconnell.github.io",
    "version": "1.0.0",
    "description": "Just my personal blog.",
    "scripts": {
        "start": "npm run dev",
        "dev": "run-s clean eleventy:dev --print-label",
        "build": "run-s clean eleventy:prod --print-label",
        "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "eleventy:debug": "cross-env ELEVENTY_ENV=development DEBUG=Eleventy* eleventy --serve",
        "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "del-cli dist",
        "lint": "npx prettier --check .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/umcconnell/umcconnell.github.io"
    },
    "author": "Ulysse McConnell",
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy": "^1.0.1",
        "@babel/core": "^7.17.10",
        "@babel/plugin-transform-runtime": "^7.17.10",
        "@babel/preset-env": "^7.16.11",
        "@iktakahiro/markdown-it-katex": "^4.0.1",
        "babel-loader": "^8.2.5",
        "clean-css": "^5.3.0",
        "critical": "^4.0.1",
        "cssesc": "^3.0.0",
        "del-cli": "^4.0.1",
        "focus-trap": "^6.9.0",
        "focus-visible": "^5.2.0",
        "html-minifier": "^4.0.0",
        "luxon": "^2.3.1",
        "markdown-it": "^13.0.0",
        "markdown-it-anchor": "^8.4.1",
        "markdown-it-footnote": "^3.0.3",
        "memfs": "^3.4.1",
        "node-sass": "^7.0.1",
        "npm-run-all": "^4.1.5",
        "svg-sprite": "^1.5.4",
        "webpack": "^5.72.0"
    },
    "devDependencies": {
        "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
        "@11ty/eleventy-plugin-rss": "^1.1.2",
        "@11ty/eleventy-navigation": "^0.3.2",
        "cross-env": "^7.0.3",
        "prettier": "2.6.2"
    }
}
