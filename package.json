{
    "name": "umcconnell.github.io",
    "version": "1.0.0",
    "description": "Just my personal blog.",
    "scripts": {
        "start": "npm run dev",
        "dev": "run-s clean eleventy:dev --print-label",
        "build": "run-s clean eleventy:prod --print-label",
        "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "eleventy:debug": "cross-env ELEVENTY_ENV=development DEBUG=Eleventy* eleventy --serve",
        "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "del-cli dist",
        "lint": "npx prettier --check .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/umcconnell/umcconnell.github.io"
    },
    "author": "Ulysse McConnell",
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy": "^1.0.2",
        "@babel/core": "^7.20.7",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.20.2",
        "@iktakahiro/markdown-it-katex": "^4.0.1",
        "babel-loader": "^9.1.0",
        "clean-css": "^5.3.1",
        "critical": "^5.0.4",
        "cssesc": "^3.0.0",
        "del-cli": "^5.0.0",
        "focus-trap": "^7.2.0",
        "focus-visible": "^5.2.0",
        "html-minifier": "^4.0.0",
        "luxon": "^3.2.1",
        "markdown-it": "^13.0.1",
        "markdown-it-anchor": "^8.6.6",
        "markdown-it-footnote": "^3.0.3",
        "memfs": "^3.4.12",
        "node-sass": "^8.0.0",
        "npm-run-all": "^4.1.5",
        "svg-sprite": "^2.0.2",
        "webpack": "^5.75.0"
    },
    "devDependencies": {
        "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
        "@11ty/eleventy-plugin-rss": "^1.2.0",
        "@11ty/eleventy-navigation": "^0.3.5",
        "cross-env": "^7.0.3",
        "prettier": "2.8.1"
    }
}
