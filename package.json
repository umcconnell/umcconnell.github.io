{
    "name": "umcconnell.github.io",
    "version": "1.0.0",
    "description": "Just my personal blog.",
    "scripts": {
        "start": "npm run dev",
        "dev": "run-s clean eleventy:dev --print-label",
        "build": "run-s clean eleventy:prod --print-label",
        "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "eleventy:debug": "cross-env ELEVENTY_ENV=development DEBUG=Eleventy* eleventy --serve",
        "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "del-cli dist",
        "lint": "npx prettier --check .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "imports": {
        "#src/*": "./src/*",
        "#utils/*": "./utils/*"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/umcconnell/umcconnell.github.io"
    },
    "type": "module",
    "author": "Ulysse McConnell",
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy": "3.1.2",
        "@babel/core": "^7.28.3",
        "@babel/plugin-transform-runtime": "^7.27.4",
        "@babel/preset-env": "^7.28.3",
        "@babel/runtime": "^7.28.3",
        "@iktakahiro/markdown-it-katex": "^4.0.1",
        "babel-loader": "^10.0.0",
        "clean-css": "^5.3.3",
        "critical": "^7.2.1",
        "cssesc": "^3.0.0",
        "del-cli": "^6.0.0",
        "focus-trap": "^7.6.5",
        "focus-visible": "^5.2.1",
        "fuse.js": "^7.1.0",
        "html-minifier": "^4.0.0",
        "luxon": "^3.7.1",
        "markdown-it": "^14.1.0",
        "markdown-it-anchor": "^9.2.0",
        "markdown-it-footnote": "^4.0.0",
        "markdown-it-table-of-contents": "^0.9.0",
        "memfs": "^4.38.2",
        "npm-run-all": "^4.1.5",
        "sass": "^1.93.2",
        "svg-sprite": "^2.0.4",
        "webpack": "^5.101.3"
    },
    "devDependencies": {
        "@11ty/eleventy-navigation": "^1.0.4",
        "@11ty/eleventy-plugin-rss": "^2.0.4",
        "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
        "cross-env": "^10.0.0",
        "prettier": "3.6.2"
    }
}
